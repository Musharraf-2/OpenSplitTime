<%# locals: (event:, presenter:) %>

<%= turbo_stream.update(dom_id(event, :sync_preview),
                        partial: "events/connectors/services/sync_preview",
                        locals: {
                          presenter: presenter,
                        }) %>

<%= turbo_stream.update(dom_id(event, :sync_or_preview_button),
                        partial: "events/connectors/services/sync_button",
                        locals: {
                          presenter: presenter,
                        }) %>

<%= turbo_stream.update(dom_id(event, :sync_efforts_response),
                        partial: "events/connectors/services/sync_efforts_response",
                        locals: {
                          interactor_response: presenter.preview_response,
                        }) %>
