<%# locals: (event:, split:, aid_station: -%>

<tr id="<%= dom_id(split, dom_id(event)) %>">
  <td><%= render "events/aid_station_checkbox", event: event, split: split, aid_station: aid_station %></td>
  <td><strong><%= split.base_name %></strong></td>
  <td><%= "#{pdu('singular').titleize} #{d(split.distance_from_start)}" %></td>
  <td><%= "#{e(split.vert_gain_from_start)} #{peu}" if split.vert_gain_from_start?  %></td>
  <td><%= "#{e(split.vert_loss_from_start)} #{peu}" if split.vert_loss_from_start?  %></td>
  <td><%= "#{e(split.elevation)} #{peu}" if split.elevation? %></td>
  <td><%= "#{split.latitude} / #{split.longitude}" if split.latitude? && split.longitude? %></td>
  <td>

  </td>
</tr>
