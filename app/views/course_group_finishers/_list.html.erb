<div>
  <table class="table table-striped">
    <thead>
    <tr>
      <th><%= link_to_sort_heading("Name", "last_name,first_name") %></th>
      <th><%= link_to_sort_heading("Gender", "gender,last_name,first_name") %></th>
      <th><%= link_to_sort_heading("From", "state_code,last_name,first_name") %></th>
      <th class="text-center"><%= link_to_sort_heading("Finish Count", "-finish_count,last_name,first_name") %></th>
    </tr>
    </thead>
    <tbody id="course_group_finishers">
    <%= render partial: "course_group_finisher",
               collection: @presenter.filtered_finishers,
               locals: { organization: @presenter.organization, course_group: @presenter.course_group } %>
    </tbody>
  </table>

  <%= render "shared/pager", next_page_url: @presenter.next_page_url %>
</div>
