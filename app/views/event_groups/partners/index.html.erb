<% content_for :title do %>
  <% "OpenSplitTime: Event Group Partners - #{@presenter.event_group.name}" %>
<% end %>

<%= render "shared/mode_widget", event_group: @presenter.event_group %>
<%= render "event_groups/setup_header", presenter: @presenter, breadcrumbs: ["Partners"] %>

<article class="ost-article container">
  <div class="row flex-nowrap">
    <%= render "event_groups/setup_sidebar", event_group: @presenter.event_group %>
    <div class="col px-md-4 py-3">
      <%= render "partner_list", presenter: @presenter %>
    </div>
  </div>
</article>
