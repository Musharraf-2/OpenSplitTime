<%# locals: (presenter:, event_series:) -%>

<table class="table table-centered table-fixed-sm table-striped">
  <thead>
  <tr>
    <th>Name</th>
    <th>Dates</th>
    <th></th>
  </tr>
  </thead>
  <tbody>
  <% event_series.each do |series| %>
      <tr>
        <td><%= link_to series.name, organization_event_series_path(presenter.organization, series) %></td>
        <td><%= presenter.event_date_range(series) %></td>
      </tr>
  <% end %>
  </tbody>
</table>
