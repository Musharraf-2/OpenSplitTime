<table class="table table-centered table-fixed-sm table-striped">
  <thead>
  <tr>
    <th>Name</th>
    <th>Courses</th>
  </tr>
  </thead>
  <tbody>
  <% @presenter.course_groups.each do |course_group| %>
    <tr>
      <td><%= link_to course_group.name, organization_course_group_path(course_group.organization, course_group) %></td>
      <td><%= course_group.courses.pluck(:name).join(", ") %></td>
    </tr>
  <% end %>
  </tbody>
</table>
