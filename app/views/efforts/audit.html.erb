<% content_for :title do %>
  <% "OpenSplitTime: Audit effort - #{@presenter.full_name} - #{@presenter.event_name}" %>
<% end %>

<% content_for(:container_type) do %>skip
<% end %>

<header class="ost-header">
  <div class="container">
    <div class="ost-heading row">
      <div class="col">
        <div class="ost-title">
          <h1><strong>
            <% if @presenter.person %>
              <%= link_to @presenter.full_name, person_path(@presenter.person) %>
            <% else %>
              <%= @presenter.full_name %>
            <% end %>
          </strong></h1>
          <%= effort_view_breadcrumbs(@presenter, 'Audit') %>
        </div>
        <div class="ost-subtitle">
          <h6>
            <strong>Status:</strong>
            <%= @presenter.finish_status %>
            <%= "â€¢ #{@presenter.overall_rank.ordinalize} Place " if @presenter.overall_rank %>
            <%= "â€¢ #{@presenter.gender_rank.ordinalize} #{@presenter.gender.titleize}" if @presenter.gender_rank %>
            <%= "â€¢ Bib ##{@presenter.bib_number}" if @presenter.bib_number %>
          </h6>
        </div>
      </div>
    </div>
    <%= effort_view_tabs(@presenter) %>
  </div>
</header>

