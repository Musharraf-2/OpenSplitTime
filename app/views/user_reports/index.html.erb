<% content_for :title do %>
  <% "OpenSplitTime: User Reports - #{current_user.full_name}" %>
<% end %>

<%= turbo_stream_from current_user %>

<header class="ost-header">
  <div class="container">
    <div class="ost-heading row">
      <div class="col">
        <div class="ost-title">
          <h1><strong>User Reports</strong></h1>
          <ul class="breadcrumb">
            <li class="breadcrumb-item"><%= link_to current_user.full_name, user_path(current_user) %></li>
            <li class="breadcrumb-item active">User Reports</li>
          </ul>
        </div>
      </div>
    </div>
  </div>
</header>

<article class="ost-article container">
  <div id="user_reports">
    <%= render partial: "user_report", collection: user_reports %>
  </div>
</article>
